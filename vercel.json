{
  "version": 2,
  "builds": [
    {
      "src": "fro/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "fro/dist" }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/$1.js" },
    { "src": "/(.*)", "dest": "fro/dist/index.html" }
  ]
}
